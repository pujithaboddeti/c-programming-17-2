#include<stdio.h>
int main()
{
    int i,n=20,k=5,s=0,l=0;
    for(i=1;i<=n;i++)
    {
        if(i%k==0)
        {
            printf("\n%d",i);

        }
    }
    for(i=1;i<=n;i++)
    {
        if(i%2==0)
        {
           if(i%k==0)
           {
               s=s+i;
           }
        }
    }
        printf("\nevensum:%d",s);
    for(i=1;i<=n;i++)
    {
        if(i%2==1)
        {
           if(i%k==0)
           {
               l=l+i;
           }
        }
    }
        printf("\noddsum:%d",l);
}

